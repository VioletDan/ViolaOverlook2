/**标准16:9*/
.article { }
.content {
  width: 100%;
  height: calc(100% - 70rpx);
  background-color: #f9f9f9;
  /* overflow: hidden; */
  /* overflow-y: scroll; */
  -webkit-overflow-scrolling: touch;
}
.move_cont{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
}
movable-area{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;

}
movable-view{
    width:121rpx;
    height:128rpx;
    z-index: 11;

}
.icon_write{
    width:121rpx;
    height:128rpx;
    position: absolute;

}
.seacrchBox {
  width: 706rpx;
  overflow: hidden;
  margin: 0 auto;
  height: auto;
  border-top: 1rpx solid transparent;
}

.seacrchBox.sActive {
  height: 100%;
}

/* ------------------------------------------------------searchInput */

.seacrchBox .searchInput {
  width: 100%;
  height: 53rpx;
  border: 1rpx solid #cb0937;
  border-radius: 23rpx;
  position: relative;
  margin-top: 19rpx;
}

.seacrchBox .searchInput input.weui-input {
  width: 80%;
  height: 100%;
  font-size: 28rpx;
  padding: 0 22rpx;
}

.iconSearch {
  width: 61rpx;
  height: 32rpx;
  position: absolute;
  top: 50%;
  margin-top: -16rpx;
  right: 30rpx;
  z-index: 10;
  background-image: url(https://clarins.beats-digital.com/wxapp/images/grassSharing/icon_search.png?sign=c59d2f414254389e53a98b7fe83baeaf&t=15506442990);
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-origin: padding-box;
  background-clip: padding-box;
  padding: 0;
  border-radius: 0;
}

button[type=primary] {
  background-color: transparent;
}

.iconSearch:after {
  border-radius: 0;
  border: none;
  padding: 0;
}

.searchKeywords {
  width: 100%;
  height: auto;
  background-color: #f9f9f9;
  /* border-top: 1rpx solid transparent; */
  text-align: center;
  position: relative;
}

/* ------------------------------------------------------classWords */

.searchKeywords .classWords {
  font-size: 28rpx;
  color: #a9a9a9;
}

.searchKeywords .classWords .txt {
  padding: 15rpx;
  display: inline-block;
  /* margin-left: 5rpx; */
  position: relative;
}

.searchKeywords .classWords .txt::after {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
  content: '|';
}

.searchKeywords .classWords .txt:last-child::after {
  content: '';
}

.searchKeywords .classWords .txt.active {
  background-color: #fff;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
}

.searchKeywords .classWords .more {
  position: relative;
  text-align: left;
  padding: 10rpx 20rpx 15rpx 20rpx;
  width: 120rpx;
}

.searchKeywords .classWords .more::before {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 8rpx;
  content: '';
  height: 0px;
  width: 0px;
  border-top: solid 10rpx #a9a9a9;
  border-left: solid 10rpx rgba(0, 0, 0, 0);
  border-right: solid 10rpx rgba(0, 0, 0, 0);
}

/* ------------------------------------------------------searchKeywords classWordsMore */

.searchKeywords .classWordsMore {
  width: 100%;
  background-color: #fff;
  height: 380rpx;
  -moz-box-shadow: 0px 3px 5px #efefef;
  -webkit-box-shadow: 0px 3px 5px #efefef;
  box-shadow: 0px 3px 5px #efefef;
  text-align: left;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.searchKeywords .classWordsMore .citem {
  margin-bottom: 35rpx;
  width: 100%;
  padding-left: 52rpx;
}

.searchKeywords .classWordsMore .citem:last-child {
  margin-bottom: 0;
}

.searchKeywords .classWordsMore .citem .citemTitle {
  color: #cb0937;
  font-size: 28rpx;
  padding-left: 12rpx;
  margin-bottom: 8rpx;
}

.searchKeywords .classWordsMore .citem .ctxt {
  color: #a9a9a9;
  font-size: 28rpx;
  padding: 0 13rpx;
  height: 30rpx;
  line-height: 30rpx;
  display: inline-block;
  text-align: center;
  position: relative;
}
.searchKeywords .classWordsMore .citem .ctxt.active{
  color: #cb0937;
}
.searchKeywords .classWordsMore .citem .ctxt::after {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
  content: '|';
  height: 30rpx;
}

.searchKeywords .classWordsMore .citem .ctxt:last-child::after {
  content: "";
}

/* ------------------------------------------------------s-history */

.s-history .s-historyTitle {
  font-size: 28rpx;
  color: #a9a9a9;
  margin-top: 20rpx;
  position: relative;
}

.s-history .s-historyTitle .icon-delete {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
  height: 32rpx;
  width: 32rpx;
  background-image: url(https://clarins.beats-digital.com/wxapp/images/grassSharing/icon-delete.png?sign=cb1357e54c4c1212a309b1460625a67d&t=1550656550);
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-origin: padding-box;
  background-clip: padding-box;
}

.s-history .stxt {
  display: inline-block;
  padding: 0.4rem 0.6rem;
  max-width: 100%;
  overflow: hidden;
  margin: 20rpx 20rpx 0 0;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  color: #333;
  -moz-border-radius: 10rpx;
  border-radius: 30rpx;
  background-color: #f5f5f5;
}

.s-history .noHistory {
  width: 100%;
  font-size: 24rpx;
  text-align: center;
  line-height: 2;
  color: #999;
  margin-top: 20rpx;
}

.searchKeywords.active {
  height: 100rpx;
}

/* ----------------------------------------------noKeywords */

.noKeywords {
  margin-top: 150rpx;
  text-align: center;
}

.noKeywords .txt {
  font-size: 28rpx;
  color: #a9a9a9;
  margin-bottom: 135rpx;
}

.recommendKeywords .title {
  font-size: 28rpx;
  color: #a9a9a9;
  margin-bottom: 10rpx;
}

.recommendKeywords .body {
  font-size: 28rpx;
  color: #cb0937;
}

