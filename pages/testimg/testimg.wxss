/* -------------------------------------------list */
.list{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.list .listItem{
  display: flex;
  height: 370rpx;
  border-left: 1rpx solid #f2f2f2;
  border-right: 1rpx solid #f2f2f2;
  border-bottom: 1rpx solid #f2f2f2;
  border-top: 2rpx solid #f2f2f2;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  flex-direction: row;
  margin-bottom: 15rpx;
  width: 100%;
}

.list .listItem .listItem-top{
  width: 420rpx;
  height: 100%;
  overflow: hidden;
  background-color: #fff;
}
.list .listItem .listItem-top>image{
  width: 100%;
  height: 100%;
  transition: all 0.2s ease-in-out;
  opacity: 0;
}
.list .listItem .listItem-top>image.active {
  opacity: 1;
}
.list .listItem .listItem-bottom{
 background-color: #fff;
 padding: 0 24rpx;
 height: 100%;
 /* width: calc(100%-250rpx); */
 width: 390rpx;
}
.list .listItem .listItem-bottom .top{
  display: flex;
  height: 69rpx;
  border-bottom: 1rpx solid #d3d3d3;
  justify-content: space-between;
  align-items: center;
}
.list .listItem .listItem-bottom .top .authorinfo{
  display: flex;
  justify-content: center;
  align-items: center;
}
.list .listItem .listItem-bottom .authorinfo .avatarUrl{
  width: 49rpx;
  height: 49rpx;
  border-radius: 50%;
}
.list .listItem .listItem-bottom .authorinfo .nickName{
  /* color: #161616; */
  font-weight:600;
  font-size: 20rpx;
  margin-left: 14rpx;
  max-width: 140rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list .listItem .listItem-bottom .userCollection{
  width: 33rpx;
  height: 33rpx;
  background-image: url(https://clarins.beats-digital.com/wxapp/images/grassSharing/icon_collect.png?sign=738984f103a1659964a6493ab220d99f&t=15508555666);
}
.list .listItem .listItem-bottom .userCollection.active{
  background-image: url(https://clarins.beats-digital.com/wxapp/images/grassSharing/icon_collect_d.png?sign=65791b1975e0c67f2064937c128f928c&t=1550827890);
}
.list .listItem .listItem-bottom .body{
  height: calc(100%-120rpx);
  width: 100%;
}
.list .listItem .listItem-bottom .body .listItemTitle{
  /* color: #161616; */
  font-weight:600;
  font-size: 25rpx;
  text-align: left;
  max-width: 100%;
  width: 100%;
  overflow: hidden;
  margin-top: 14rpx;
  height: 175rpx;
  max-height: 175rpx;
  letter-spacing: 1rpx;
  line-height: 1.4;
}
.list .listItem .listItem-bottom .body .listItemTag{
  text-align:  center;
  display: flex;
  justify-content:  space-between;
  width: 100%;
  margin: 0 auto 25rpx;
}
.list .listItem .listItem-bottom .body .listItemTag>.tag{
  width: 86rpx;
  height: 34rpx;
  background-color: #cb0937;
  color: #fff;
  font-size: 20rpx;
  line-height: 34rpx;
  display: inline-block;
  border-radius: 20rpx;
}
.list .listItem .listItem-bottom .footer{
  border-top: 1rpx solid #e4e4e4;
  height: 49rpx;
  display: flex;
  width: 100%;
  justify-content: space-between;
  align-items: center;
}
.list .listItem .listItem-bottom .footer .userLike{
  display: flex;
  justify-content: left;
  align-items: center;
  height: 100%;
  /* width: 100rpx; */
  /* max-width: 120rpx; */
}
.list .listItem .listItem-bottom .footer .userLike .icon{
  width: 30rpx;
  height: 27rpx;
  background-image: url(https://clarins.beats-digital.com/wxapp/images/grassSharing/icon_like.png?sign=b4e247ea42fcedbb86f6a98b2dc4b301&t=1550720120);
  margin-right: 8rpx;
}
.list .listItem .listItem-bottom .footer .userLike.active .icon{
  background-image: url(https://clarins.beats-digital.com/wxapp/images/grassSharing/icon_like_d.png?sign=56595aa1f975f875f38692d4c38de169&t=1550726889);
}
.list .listItem .listItem-bottom .footer  .num{
  color: #161616;
  font-size: 20rpx;
}
.list .listItem .listItem-bottom .footer .userRead{
  display: flex;
  justify-content: left;
  align-items: center;
  /* width: 100rpx; */
  /* max-width: 120rpx; */
}
.list .listItem .listItem-bottom .footer .userRead .icon{
  width: 30rpx;
  height: 20rpx;
  background-image: url(https://clarins.beats-digital.com/wxapp/images/grassSharing/icon_read.png?sign=c25e8271154093b57497401608d830a8&t=1550727197);
  margin-right: 8rpx;
}
.list .listItem .listItem-bottom .footer .userShare {
  width: 33rpx;
  height: 33rpx;
  padding: 0;
  border:none;
  background-color: transparent;
  background-image: url(https://clarins.beats-digital.com/wxapp/images/grassSharing/icon_share.png?sign=da01cb03b0bce3b0caaa27acc22e09b8&t=1550726745);
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-origin: padding-box;
  background-clip: padding-box;
}
.list .listItem .listItem-bottom .footer .userShare::after{ 
  border: none; 
} 

.list .listItem.active{
  height: auto;
  width: 49%;
  flex-direction: column;
}
.list .listItem.active .listItem-top {
  height: 400rpx;
  width: 100%;
}
.list .listItem.active .listItem-bottom{
  height: auto;
  width: auto;
  min-width: 100%;
  box-sizing: border-box;
}

